{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.blaze": {
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.body" },
        { "head": "Material.head" }
      ],
      "textures": [ "q.variant ? array.xskins[q.variant - 1] : array.xskins[variable.xvar]" ],
      "arrays": {
        "textures": {
          "array.xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8"
          ]
        }
      }
    }
  }
}
