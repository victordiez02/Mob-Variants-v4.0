{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.iron_golem": {
      "materials": [ { "*": "Material.default" } ],
      "geometry": "Geometry.default",
      "textures": [ "q.variant ? array.xskins[q.variant - 1] : array.xskins[variable.xvar]" ],
      "arrays": {
        "textures": {
          "array.xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8",
            "Texture.default9",
            "Texture.default10",
            "Texture.default11",
            "Texture.default12",
            "Texture.default13",
            "Texture.default14",
            "Texture.default15",
            "Texture.item_sorter"
          ]
        }
      }
    },
    "controller.render.irongolem_rose": {
      "rebuild_animation_matrices": true,
      "geometry": "q.variant ? (q.mark_variant ? geometry.rose : geometry.empty) : (variable.xvar < 30 ? geometry.rose : geometry.empty)",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "texture.default2" ]
    }
  }
}
