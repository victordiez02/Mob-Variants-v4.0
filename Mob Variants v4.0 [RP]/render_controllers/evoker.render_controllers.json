{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.evoker": {
      "geometry": "Geometry.default",
      "materials": [{ "*": "Material.default" }],
      "textures": [
        "q.variant ? array.xskins[q.variant - 1] : array.xskins[variable.xvar]"
      ],
      "arrays": {
        "textures": {
          "array.xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8",
            "Texture.default9",
            "Texture.default10",
            "Texture.default11",
            "Texture.default12",
            "Texture.default13",
            "Texture.default14",
            "Texture.default15",
            "Texture.default16",
            "Texture.default17",
            "Texture.default18",
            "Texture.default19",
            "Texture.default20",
            "Texture.default21",
            "Texture.default22",
            "Texture.default23",
            "Texture.default24",
            "Texture.default25",
            "Texture.default26",
            "Texture.default27",
            "Texture.default28",
            "Texture.default29",
            "Texture.default30",
            "Texture.default31",
            "Texture.default32"
          ]
        }
      }
    },
    "controller.render.evoker_hat": {
      "rebuild_animation_matrices": true,
      "geometry": "q.variant ? array.geosq[q.mark_variant] : (variable.xvar < 30 ? array.geosd[variable.xvar] : geometry.hat)",
      "materials": [{ "*": "Material.glow" }],
      "textures": [
        "q.variant ? array.xskinsq[q.mark_variant] : (variable.xvar < 60 ? (variable.xvar < 30 ? array.dskins[variable.xvar] : array.xskins[variable.xvar]) : texture.no_hat)"
      ],
      "arrays": {
        "textures": {
          "array.xskins": ["Texture.hat", "Texture.hat2", "Texture.hat3"],
          "array.dskins": [
            "Texture.wizard",
            "Texture.windcaller",
            "Texture.enchanter",
            "Texture.geomancer",
            "Texture.chef"
          ],

          "array.xskinsq": [
            "Texture.no_hat", //"texture.costume",
            "Texture.hat",
            "Texture.hat2",
            "Texture.hat3",
            "Texture.wizard",
            "Texture.windcaller",
            "Texture.enchanter",
            "Texture.geomancer",
            "Texture.chef"
          ]
        },
        "geometries": {
          "array.geosd": [
            "geometry.wizard",
            "geometry.windcaller",
            "geometry.enchanter",
            "geometry.geomancer",
            "geometry.chef"
          ],

          "array.geosq": [
            "geometry.hat", //"geometry.costume",
            "geometry.hat",
            "geometry.hat",
            "geometry.hat",
            "geometry.wizard",
            "geometry.windcaller",
            "geometry.enchanter",
            "geometry.geomancer",
            "geometry.chef"
          ]
        }
      }
    }
  }
}
