{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.guardian": {
      "geometry": "Geometry.default",
      "materials": [ { "*": "q.is_ghost ? Material.ghost : Material.default" } ],
      "textures": [ "q.is_elder ? Texture.elder : (q.variant ? array.xskins[q.variant - 1] : array.xskins[variable.xvar])" ],
      "arrays": {
        "textures": {
          "array.xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3",
            "Texture.default4",
            "Texture.default5",
            "Texture.default6",
            "Texture.default7",
            "Texture.default8",
            "Texture.default9"
          ]
        }
      }
    }
  }
}
